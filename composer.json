{
  "require": {
    "aelvan/imager": "v2.4.0",
    "biglotteryfund/preview-button": "^1.0",
    "craftcms/cms": "^3.0.0",
    "ether/seo": "^3.6",
    "fortrabbit/craft-copy": "^1.0.0-RC10",
    "spicyweb/craft-embedded-assets": "2.2.1",
    "vlucas/phpdotenv": "^3.4.0"
  },
  "autoload": {
    "psr-4": {
      "modules\\": "modules/"
    }
  },
  "config": {
    "sort-packages": true,
    "optimize-autoloader": true
  },
  "scripts": {
    "post-root-package-install": [
      "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
    ]
  }
}
